{
    "framework": "vite",
    "images": {
      "sizes": [256, 384, 640, 750, 828, 1080, 1200, 1920, 2048, 3840],
      "domains": ["vercel.app"],
      "minimumCacheTTL": 60,
      "formats": ["image/webp", "image/avif"]
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "github": {
      "silent": true
    }
} 
